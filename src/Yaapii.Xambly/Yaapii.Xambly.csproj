<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>ICARUS Consulting GmbH</Authors>
    <Copyright>ICARUS Consulting GmbH</Copyright>
    <RepositoryUrl>https://github.com/icarus-consulting/Yaapii.Xml.Xambly</RepositoryUrl>
    <DocumentationFile>artifacts\yaapii.xml.xambly.xml</DocumentationFile>
    <PackageTags>
      xml-editor, csharp, xml-documents, xpath, xml-builder
    </PackageTags>
    <PackageProjectUrl>https://github.com/icarus-consulting/Yaapii.Xml.Xambly</PackageProjectUrl>
    <PackageReleaseNotes>https://github.com/icarus-consulting/Yaapii.Xml.Xambly/releases</PackageReleaseNotes>
    <DocumentationFile>$(MSBuildThisFileDirectory)bin\$(Configuration)\$(TargetFramework)\Yaapii.Xambly.xml</DocumentationFile>
  </PropertyGroup>
  
  <ItemGroup>
    <None Remove="Yaapii.Xambly.nuspec" />
    <None Remove="Yaapii.Xambly.Sources.nuspec" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="System.Xml.XmlDocument" Version="4.3.0" />
    <PackageReference Include="System.Xml.XPath.XmlDocument" Version="4.3.0" />
    <PackageReference Include="Antlr4.Runtime.Standard" Version="4.7.1.1" />
    <PackageReference Include="Yaapii.Atoms" Version="2.1.3" />
  </ItemGroup>
</Project>
