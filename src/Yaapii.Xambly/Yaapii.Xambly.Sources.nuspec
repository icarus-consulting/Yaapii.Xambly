<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <!-- The required element 'version' is set in the cake build script -->
    <id>Yaapii.Xambly</id>
    <description>Port of Xembly Library from Yegor Bugayenko.</description>
    <authors>ICARUS Consulting GmbH</authors>

    <owners>ICARUS Consulting GmbH</owners>
    <copyright>Copyright (c) 2020 ICARUS Consulting GmbH</copyright>
    <projectUrl>https://github.com/icarus-consulting/Yaapii.Xambly</projectUrl>
    <license type="file">LICENSE.txt</license>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <repository type="git" url="https://github.com/icarus-consulting/Yaapii.Xambly.git" />
    <tags>xml-editor, csharp, xml-documents, xpath, xml-builder</tags>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Antlr4.Runtime.Standard" version="4.7.1.1" exclude="Build,Analyzers" />
        <dependency id="System.Xml.XPath.XmlDocument" version="4.3.0" exclude="Build,Analyzers" />
        <dependency id="System.Xml.XmlDocument" version="4.3.0" exclude="Build,Analyzers" />
        <dependency id="Yaapii.Atoms.Sources" version="2.1.3" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <contentFiles>
      <files include="cs\netstandard2.0\LIBS\Yaapii.Xambly\**\*.cs" buildAction="content" flatten="true" copyToOutput="true" />
    </contentFiles>
  </metadata>
  <files>
    <file src="LICENSE.txt" target=""/>
    <file src="src\Yaapii.Xambly\**\*.cs" exclude="src\Yaapii.Xambly\obj\**\*.*" target="contentFiles\cs\netstandard2.0\LIBS" />
    <file src="src\Yaapii.Xambly\**\*.cs" exclude="src\Yaapii.Xambly\obj\**\*.*" target="content\LIBS" />
  </files>
</package>